{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "inStock": {
      "type": "integer",
      "min": 1,
      "default": 1,
      "required": true
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "computer-all",
        "audio-all",
        "mobile-all",
        "tv-video-all"
      ],
      "required": true
    },
    "discountPercent": {
      "type": "integer"
    },
    "hasDiscount": {
      "type": "boolean",
      "default": false
    },
    "Features": {
      "type": "json"
    },
    "variants": {
      "type": "json",
      "required": true
    },
    "storage": {
      "type": "json"
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": false
    },
    "sub_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::sub-category.sub-category",
      "inversedBy": "products"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "products"
    }
  }
}
