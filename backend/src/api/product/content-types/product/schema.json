{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "inStock": {
      "type": "integer",
      "min": 1,
      "default": 1,
      "required": true
    },
    "price": {
      "type": "float",
      "required": true
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "laptop",
        "desktop",
        "cell-phones",
        "tablets",
        "ear-buds"
      ],
      "required": true
    },
    "discountPercent": {
      "type": "integer"
    },
    "hasDiscount": {
      "type": "boolean",
      "default": false
    },
    "variants": {
      "type": "json",
      "required": true
    },
    "storage": {
      "type": "json"
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": false
    },
    "sub_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::sub-category.sub-category",
      "inversedBy": "products"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "OS": {
      "type": "string",
      "required": false
    },
    "manufacturer": {
      "type": "string",
      "required": false
    },
    "brand": {
      "type": "string",
      "required": true
    },
    "ram": {
      "type": "string"
    },
    "model_name": {
      "type": "string"
    },
    "wireless_carrier": {
      "type": "string"
    },
    "sim_card_slot_count": {
      "type": "string"
    },
    "sim_card_size": {
      "type": "string"
    },
    "Network_Technology": {
      "type": "string"
    },
    "connectivity_technologies": {
      "type": "string"
    },
    "battery_description": {
      "type": "string"
    },
    "rear_camera": {
      "type": "string"
    },
    "front_camera": {
      "type": "string"
    },
    "biometric_security_feature": {
      "type": "string"
    },
    "water_resistant": {
      "type": "string"
    },
    "hard_drive_size": {
      "type": "string"
    },
    "graphic_detail": {
      "type": "string"
    },
    "displaySize": {
      "type": "string"
    },
    "speed": {
      "type": "string"
    },
    "processor": {
      "type": "string"
    },
    "Weight": {
      "type": "string"
    },
    "Charging_Time": {
      "type": "string"
    },
    "Fabric_Type": {
      "type": "string"
    },
    "Closure_Type": {
      "type": "string"
    },
    "Neck_Style": {
      "type": "string"
    },
    "Sole_Material": {
      "type": "string"
    }
  }
}
